trainFileList = test-files/german.bio
type = crf
serializeTo = test-files/eunews_german.crf.gz
map = word=0,answer=2
useClassFeature=true
useWord=true
useNGrams=true
noMidNGrams=true
maxNGramLeng=6
usePrev=true
useNext=true
useSequences=true
usePrevSequences=true
maxLeft=1
useTypeSeqs=true
useTypeSeqs2=true
useTypeySequences=true
wordShape=chris2useLC
useDisjunctive=true
readerAndWriter=edu.stanford.nlp.sequences.ColumnDocumentReaderAndWriter
saveFeatureIndexToDisk = true
